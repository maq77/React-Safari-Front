{
  "version": 2,
  "buildCommand": "npm install --include=optional && npm run build",
  "installCommand": "npm install --include=optional",
  "framework": "vite",
  "rewrites": [
    { "source": "/api/:path*", "destination": "http://safari-api-mohamed.runasp.net/api/:path*" },
    { "source": "/(.*)", "destination": "/" }
  ]
}